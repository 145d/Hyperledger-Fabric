{
	"name": "test-network",
	"version": "1.0.0",
	"client": {
		"tlsEnable": true,
		"adminCredential": {
			"id": "exploreradmin",
			"password": "exploreradminpw"
		},
		"enableAuthentication": true,
		"organization": "Org1MSP",
		"connection": {
			"timeout": {
				"peer": {
					"endorser": "300"
				},
				"orderer": "300"
			}
		}
	},
	"channels": {
		"mychannel": {
			"peers": {
				"peer0.org1.example.com": {},
                                "orderer2.example.com": {}
			}
                }
	},
	"organizations": {
		"Org1MSP": {
			"mspid": "Org1MSP",
			"adminPrivateKey": {
				"path": "/tmp/crypto/peerOrganizations/org1.example.com/users/Admin@org1.example.com/msp/keystore/priv_sk"
			},
			"peers": ["peer0.org1.example.com"],
			"signedCert": {
				"path": "/tmp/crypto/peerOrganizations/org1.example.com/users/Admin@org1.example.com/msp/signcerts/Admin@org1.example.com-cert.pem"
			}
		},
                "Orderer2MSP": {
                        "mspid": "Orderer2MSP",
                        "adminPrivateKey": {
                                "path": "/tmp/crypto/ordererOrganizations/example.com/orderers/orderer2.example.com/msp/keystore/priv_sk"
                        },
                        "peers": ["orderer2.example.com"],
                        "signedCert": {
                                "path": "/tmp/crypto/ordererOrganizations/example.com/orderers/orderer2.example.com/msp/signcerts/orderer2.example.com-cert.pem"
                        }
		}
	},
	"peers": {
		"peer0.org1.example.com": {
			"tlsCACerts": {
				"path": "/tmp/crypto/peerOrganizations/org1.example.com/peers/peer0.org1.example.com/tls/ca.crt"
			},
			"url": "grpcs://peer0.org1.example.com:7051"
		},
                "peer0.org2.example.com": {
                        "tlsCACerts": {
                                "path": "/tmp/crypto/peerOrganizations/org2.example.com/peers/peer0.org2.example.com/tls/ca.crt"
                        },  
                        "url": "grpcs://peer0.org2.example.com:9051"
                },  
		"orderer.example.com": {
			"tlsCACerts": {
                                "path": "/tmp/crypto/ordererOrganizations/example.com/orderers/orderer.example.com/tls/ca.crt"
			},
			"url": "grpcs://orderer.example.com:7050"
		},
                "orderer2.example.com": {
                        "tlsCACerts": {
                                "path": "/tmp/crypto/ordererOrganizations/example.com/orderers/orderer2.example.com/tls/ca.crt"
                        },
                        "url": "grpcs://orderer2.example.com:7050"
                },
                "orderer3.example.com": {
                        "tlsCACerts": {
                                "path": "/tmp/crypto/ordererOrganizations/example.com/orderers/orderer3.example.com/tls/ca.crt"
                        },
                        "url": "grpcs://orderer3.example.com:7050"
                }
        }
}
